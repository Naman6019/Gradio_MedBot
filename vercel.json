{
  "version": 2,
  "builds": [
    {
      "src": "app.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "app.py"
    }
  ],
  "env": {
    "PYTHON_VERSION": "3.11"
  },
  "build": {
    "command": "pip install -r requirements.txt"
  },
  "start": "uvicorn app:app --host 0.0.0.0 --port 8000"
}
